<%@ page contentType="text/html;charset=UTF-8" %>
<html>
<head>
    <title>Manage Vehicles</title>
</head>
<body>
    <h1>Vehicle Management</h1>
    <form action="AddVehicleServlet" method="post">
        Make: <input type="text" name="make" required><br>
        Model: <input type="text" name="model" required><br>
        Year: <input type="date" name="year" required><br>
        Capacity: <input type="number" name="capacity" required><br>
        Plate: <input type="text" name="plate" required><br>
        <input type="submit" value="Add Vehicle">
    </form>
</body>
</html>